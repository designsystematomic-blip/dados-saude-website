{
  "name": "dados-saude-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lib:v": "npm view design-system-atomic version",
    "link:lib": "npm link design-system-atomic",
    "unlink": "npm unlink design-system-atomic",
    "generate": "cross-env NODE_OPTIONS=\"--import tsx\" plop --plopfile=./generate/plopfile.ts",
    "update:lib": "npm install design-system-atomic@latest"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.2",
    "design-system-atomic": "^1.0.13",
    "isbot": "^5.1.31",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.64.0",
    "react-router": "^7.9.2",
    "remix-utils": "^9.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "cross-env": "^10.1.0",
    "plop": "^4.0.4",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
